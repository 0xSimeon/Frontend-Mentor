{
	"name": "natours",
	"version": "1.0.0",
	"description": "Landind page challenge by FrondMentor ",
	"main": "index.js",
	"scripts": {
		"watch:sass": "node-sass sass/main.scss css/styles.css -w",
		"devserver": "live-server",
		"start": "npm-run-all --parallel devserver watch:sass",
		"compile:sass": "node-sass sass/main.scss css/styles.comp.css",
		"compress:css": "node-sass css/styles.comp.css css/styles.css --output-style compressed",
		"build:css": "npm-run-all compile:sass   compress:css"
	},
	"author": "Simeon Udoh",
	"license": "ISC",
	"devDependencies": {
		"autoprefixer": "^7.1.4",
		"concat": "^1.0.3",
		"node-sass": "^4.13.1",
		"npm-run-all": "^4.1.1",
		"postcss-cli": "^4.1.1"
	}
}
